<Page
    x:Class="UnoOnnxSamples.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UnoOnnxSamples"
	xmlns:views="clr-namespace:UnoOnnxSamples.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"    
	Padding="0,50"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

	<NavigationView PaneDisplayMode="Top"
					IsSettingsVisible="False"
					IsBackButtonVisible="Collapsed"				
					SelectionChanged="NavigationView_SelectionChanged">
		<NavigationView.MenuItems>
			<NavigationViewItem Content="Onnx"
								Icon="NewFolder"
								Tag="PageOne" />
			<NavigationViewItem Content="PyTorch"
								Icon="Document"
								Tag="PageTwo" />
			<NavigationViewItem Content="TF Lite"
								Icon="Library"
								Tag="PageThree" />
			<NavigationViewItem Content="Keras"
								Icon="Link"
								Tag="PageFour" />
		</NavigationView.MenuItems>
		<Frame x:Name="contentFrame"/>
	</NavigationView>
</Page>
